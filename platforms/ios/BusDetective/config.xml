<?xml version='1.0' encoding='utf-8'?>
<widget id="com.teamgaslight.busdetective" version="0.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CDVWebViewEngine">
        <param name="ios-package" value="CDVWebViewEngine" />
    </feature>
    <feature name="LaunchScreen">
        <param name="ios-package" value="CDVLaunchScreen" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>BusDetective</name>
    <description>Find your bus!</description>
    <author email="hello@teamgaslight.com" href="https://teamgaslight.com">Gaslight</author>
    <content src="http://app.busdetective.com" />
    <allow-navigation href="*://*.busdetective.com/*" />
    <allow-intent href="mailto:*" />
    <access origin="*" />
    <icon src="ios-icon.png" />
    <splash height="480" src="resources/splash/Default~iphone.png" width="320" />
    <splash height="960" src="resources/splash/Default@2x~iphone.png" width="640" />
    <splash height="1024" src="resources/splash/Default-Portrait~ipad.png" width="768" />
    <splash height="2048" src="resources/splash/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="1136" src="resources/splash/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="resources/splash/Default-667h.png" width="750" />
    <splash height="2208" src="resources/splash/Default-736h.png" width="1242" />
    <icon height="57" src="resources/icon/ios/icon.png" width="57" />
    <icon height="114" src="resources/icon/ios/icon@2x.png" width="114" />
    <icon height="40" src="resources/icon/ios/icon-40.png" width="40" />
    <icon height="80" src="resources/icon/ios/icon-40@2x.png" width="80" />
    <icon height="180" src="resources/icon/ios/icon-60@3x.png" width="180" />
    <icon height="60" src="resources/icon/ios/icon-60.png" width="60" />
    <icon height="120" src="resources/icon/ios/icon-60@2x.png" width="120" />
    <icon height="76" src="resources/icon/ios/icon-76.png" width="76" />
    <icon height="152" src="resources/icon/ios/icon-76@2x.png" width="152" />
    <icon height="72" src="resources/icon/ios/icon-72.png" width="72" />
    <icon height="144" src="resources/icon/ios/icon-72@2x.png" width="144" />
    <icon height="29" src="resources/icon/ios/icon-small.png" width="29" />
    <icon height="58" src="resources/icon/ios/icon-small@2x.png" width="58" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaTypesRequiringUserActionForPlayback" value="none" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="StatusBarBackgroundColor" value="#444444" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
